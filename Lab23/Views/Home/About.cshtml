@model List< Lab23.Models.Item>

    @{
        ViewBag.Title = "About";
    }

   
<table>
    <tr>
        <th></th>
        <th>Item Name</th>
        <th>Quantity</th>
        <th>Price</th>
        <th>Description</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.itemName</td>
            <td>@item.quantity</td>
            <td>@item.price</td>
            <td>@item.description</td>
        </tr>
    }
</table>